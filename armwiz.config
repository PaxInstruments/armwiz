short_description = ST Microsystems peripheral drivers for STM32F2xx chips.
description = Hardware peripherals provided by ST Microsystems for STM32F2 series microcontrollers.
gitName = STM32CubeF2
gitURL = https://github.com/PaxInstruments/STM32CubeF2.git
website = http://www.st.com/web/en/catalog/tools/PF260266

[stm32cubef3]
name = STM32CubeF3
item_type = library
option = stm32cubef3
short_description = ST Microsystems peripheral drivers for STM32F3xx chips.
description = Hardware peripherals provided by ST Microsystems for STM32F3 series microcontrollers.
gitName = STM32CubeF3
gitURL = https://github.com/PaxInstruments/STM32CubeF3.git
